<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="requestAdditionSection" type="DoctorNetIFService.Config.RequestAdditionHandler, DoctorNetIFService"/>  </configSections>
  <connectionStrings>
    <add name="DoctorNetIFService.Properties.Settings.ConnectionStringRISDB" connectionString="DATA SOURCE=RRIS;PASSWORD=RRIS1;PERSIST SECURITY INFO=True;STATEMENT CACHE SIZE=100;USER ID=RRIS" providerName="Oracle.DataAccess.Client"/>
  </connectionStrings>
  <appSettings>
    <!-- サービススレッド停止タイムアウト(ms) -->
    <add key="ThreadStopTimeout" value="200000"/>
    <!-- クライアント停止タイムアウト(ms) -->
    <add key="ClientCloseTimeout" value="90000"/>
    <!-- SQLコマンドタイムアウト(s) -->
    <add key="CommandTimeout" value="30"/>
    <!-- ループ休眠時間(ms) -->
    <add key="ThreadSleepTime" value="500"/>
    <!-- log4netファイル日付部分フォーマット log4net.configのRollingLogFileAppenderの出力ファイル名を修正した場合は合わせて修正する -->
    <add key="log4netFileDateFormat" value="yyyyMMdd"/>
    <!-- log4netファイル正規表現パターン TargetGroupは日付部分を指すので必須。log4net.configのRollingLogFileAppenderの出力ファイル名を修正した場合は合わせて修正する -->
    <add key="log4netFileRegex" value="^DoctorNetIFService\.(?&lt;TargetGroup&gt;\d{8})\.log(\.[0-9]*)?$"/>
    <!-- 過去ログファイルの保存期間(日数) -->
    <add key="LogStoreTerm" value="30"/>
	<!-- TOOTHERSYSTEMINFOの取得条件(REQUESTTYPE) ※カンマで複数指定可 -->
	<add key="TargetRequestType" value="OP01,OP02"/>
    <!-- TOOTHERSYSTEMINFOの取得条件(SYSTEMNO) -->
	<add key="TargetSystemNo" value="1"/>
	<!-- 出力先フォルダ -->
	<add key="OutputFolder" value="C:\folder\DoctorNetIFService"/>
	<!-- 一度に取得可能な未処理レコード -->
	<add key="RecordCount" value="10"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2"/>
  </startup>
  <!-- ********** ★★★★★重要★★★★★ ****************************** -->
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Oracle.DataAccess" publicKeyToken="89B483F429C47342"/>
        <!-- Oracle.DataAccessバージョン設定 -->
        <!-- newVersionには、現地環境のOracle.DataAccessバージョンを設定して下さい。 -->
        <!-- [Oracle.DataAccessバージョン確認方法] -->
        <!-- Oracleインストールディレクトリ\ODP.NET\bin\4にOracle.DataAccess.dllファイルが存在するので -->
        <!-- 右クリック → プロパティ → 詳細 → ファイルバージョン で確認して下さい。 -->
        <bindingRedirect oldVersion="4.121.2.0" newVersion="4.121.2.0"/>
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri=""/>
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400"/>
      </providers>
    </roleManager>
  </system.web>
</configuration>
